/*
format of the entity definition are

entity "name of the entity"
{
    properties
    {
        scale = (vector2 here);
        pos = (vector2 here);
    }

    states
    {
        "texture name" "tick amount";
    }
}

*/

object a
{
    
    
    texture = wot;
    time = 1;
    next_state = noll;
}

state b
{
    texture = zis;
    time = 2;
    next_state = null;
}
